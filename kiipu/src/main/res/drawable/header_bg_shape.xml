<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
    <!-- 图形填充颜色 -->
    <solid android:color="@color/white"/>

    <!-- 边界线宽度及颜色 -->
    <stroke
            android:width="3dp"
            android:color="@color/red"/>
    <!-- 圆角大小，以圆的半径表示 -->
    <corners
            android:radius="36dp"/>
</shape>